{
  "name": "meishiplus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:webpack": "next dev --webpack",
    "dev:webpack": "NODE_OPTIONS='--no-experimental-fetch' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:reset": "prisma db push --force-reset --skip-generate",
    "db:studio": "prisma studio",
    "fix:prisma": "rm -rf node_modules/.prisma node_modules/@prisma .next && npm install && npm run db:generate",
    "cleanup": "rm -rf node_modules/.prisma node_modules/@prisma/client .next && npm run db:reset && npm run db:generate && npm run db:push"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.37.1",
    "@prisma/client": "^6.19.2",
    "@types/qrcode": "^1.5.6",
    "clsx": "^2.1.1",
    "next": "16.1.6",
    "openai": "^6.17.0",
    "pg": "^8.11.3",
    "prisma": "^6.19.2",
    "qrcode": "^1.5.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "svix": "^1.84.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.10.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
